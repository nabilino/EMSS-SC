<DESIGNER id="page" phraseId="lblPortalPage" installed="y">
	<INDEXHTM>index.htm</INDEXHTM>
	<MENU>menu.xml</MENU>
	<TOOLBOX>toolbox.xml</TOOLBOX>
	<MESSAGE>msgs.xml</MESSAGE>
	<ICON>images/logo2.gif</ICON>
	<VIEWS>
		<VIEW id="design" phraseId="lblDesign" editor="WYSIWYG" src="pgdesign.htm" lwidth="15" rwidth="15" default="y"/>
		<VIEW id="source" phraseId="lblSource" editor="TEXT" src="pgsource.htm" lwidth="0" rwidth="0"/>
