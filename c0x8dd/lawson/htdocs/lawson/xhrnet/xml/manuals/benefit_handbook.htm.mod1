<!DOCTYPE html>
<html lang="en">
<head>
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=IE8">
<meta charset="utf-8">
<meta name="viewport" content="width=device-width" />
<title>Benefit Handbook</title>
<script>
// Replace this file with your English Benefits Handbook (user language = ""), or define
// an HTML handbook for another language with the name benefit_handbook_[language].htm
function writeContent()
{
	parent.setWinTitle(parent.getSeaPhrase("BENEFIT_HANDBOOK","ESS"), window);
	self.main.document.getElementById("paneHeader").innerHTML = parent.getSeaPhrase("BENEFIT_INFO","ESS");
	self.main.document.getElementById("paneBody").innerHTML = '<div class="fieldlabelboldleft" style="padding-left:10px;padding-right:10px;padding-top:10px">'+parent.getSeaPhrase("BENEFITHANDBOOK","ESS")+'</div>';
	self.main.stylePage();
}
</script>
</head>
<body onload="writeContent()">
	<iframe id="main" name="main" title="Main Content" level="2" tabindex="0" style="left:0%;width:100%;height:100%" src="/lawson/xhrnet/ui/headerpane.htm" frameborder="no" marginwidth="0" marginheight="0" scrolling="no"></iframe>
</body>
</html>
