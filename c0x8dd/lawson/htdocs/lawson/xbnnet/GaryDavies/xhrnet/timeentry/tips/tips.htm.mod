<script>

function OpenProgram()
{
	clearTimeout(Timer)
	authenticate("frameNm='jsreturn'|funcNm='OpenTips()'|desiredEdit='EM'")
}

function OpenTips()
{
	document.title = getSeaPhrase("TIPS","TE")
	var htmlBody = '';
	var htmlHeader = '<div class="dialoglabel">'
	htmlHeader += opener.getSeaPhrase("HELP","ESS")
	htmlHeader += '</div>'
	self.main.document.getElementById("paneHeader").innerHTML = htmlHeader;

	// Default help text.
	htmlBody = '<p class="plaintablecellbold">'+getSeaPhrase("DID_U_KNOW","TE")+'</p>'
// MOD BY BILAL
	//htmlBody += '<p class="plaintablecell">'+getSeaPhrase("WHERE_TO_CHANGE_TIPS","TE")+'</p>'
// END OF MOD

	// *************************************************************************************
	// HOW TO CUSTOMIZE THE HELP TEXT CONTAINED IN THIS FILE:
	// *************************************************************************************

	// Text can be added here manually:
	// (Uncomment the following line to test.)
	// htmlBody = 'Insert your help text here.';

	// Or, you can define phrases in the /lawson/xhrnet/xml/lang/enus/temsgs.xml file and reference them
	// using the getSeaPhrase() function (for example, getSeaPhrase("MY_HELP_TEXT","TE"), where "MY_HELP_TEXT"
	// is the name of the id attribute on the phrase tag in the temsgs.xml file.  Here are the steps to
	// follow:
	// 1) Add a phrase tag to the end of the temsgs.xml file, just above the </Translate> tag.  You can
	// insert any text you wish inside the phrase tag.  The end of the file should look like this
	// (without the comment symbols ("//") on each line):
	// 		<phrase id="MY_HELP_TEXT">Insert your help text here.</phrase>
	// </Translate>
	//
	// 2) Then, add that text to the body of this HTML file as a string in HTML format by creating a string
	// with the getSeaPhrase("MY_HELP_TEXT","TE") function:
	// (Uncomment the following line to test.)
	// htmlBody = '<p class="plaintablecell">'+getSeaPhrase("MY_HELP_TEXT","TE")+'</p>';
// MOD BY BILAL  - 01/25/2011
	+ 'If you need help with entering your time in myHR please call Payroll at<font style="font-family:times  roman;font-size:10pt;color:red;">&nbsp;208-381-2528.</font><br>' 
	+ '<font style="font-family:times roman;font-size:10pt;color:black;">If you are experiencing system issues call &nbsp;<font style="font-family:times roman;font-size:10pt;color:red;">208-381-HELP.<br>'
	+ '<font style="font-family:times roman;font-size:10pt;color:black;">There is a <font style="font-family:times roman;font-size:10pt;color:red;">FAQ Bookmark <font style="font-family:times roman;font-size:10pt;color:black;">under <font style="font-family:times roman;font-size:10pt;color:red;">myHR Time Entry <font style="font-family:times roman;font-size:10pt;color:black;">for your convenience.<br>'
	+ '<br>'
	+ 'Payroll Contacts:<br> '
	+ 'Payroll Help Line&nbsp;208-381-2528<br> '
	+ 'Kerri Campbell&nbsp;&nbsp;208-381-2544<br> '
	+ 'Sheryl Doughty-Otto 208-381-3789&nbsp;<br> '
	+ '</font>'
// END OF MOD
	self.main.document.getElementById("paneBody").innerHTML = htmlBody;
	self.main.stylePage();
	document.getElementById("main").style.visibility = "visible";
}

var Timer = setTimeout("OpenProgram()",3000)

</script>
